---
import Authors from "@components/publications/Authors.astro";
import type { JournalType } from "env.d.ts";
export type Props = JournalType;
const { authors, title, journal, year } = Astro.props;
---

<>
  <Authors authors={authors} />.
  <span>"{title}</span>",
  <span class="inline-block font-semibold italic">
    {journal}
  </span>
  <span>({year})</span>.
</>
